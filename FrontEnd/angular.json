{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "FrontEnd": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:component": {
                    "skipTests": true
                },
                "@schematics/angular:directive": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                },
                "@schematics/angular:interceptor": {
                    "skipTests": true
                },
                "@schematics/angular:module": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "allowedCommonJsDependencies": [
                            "crypto-js",
                            "moment",
                            "pdfmake",
                            "qrcode"
                        ],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "outputPath": "dist",
                        "polyfills": [
                            "zone.js"
                        ],
                        "scripts": [],
                        "styles": [
                            "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "node_modules/primeng/resources/primeng.min.css",
                            "node_modules/primeng/resources/themes/md-light-indigo/theme.css",
                            "node_modules/primeng/resources/themes/saga-blue/theme.css",
                            "src/assets/styles/custom/base.css",
                            {"input": "src/assets/themes/celery-stick/base-celery-stick.css", "bundleName": "celery-stick", "inject": false },
                            {"input": "src/assets/themes/celery-stick/dark-celery-stick.css", "bundleName": "dark-celery-stick", "inject": false },                            
                            {"input": "src/assets/themes/celery-stick/light-celery-stick.css", "bundleName": "light-celery-stick", "inject": false },
                            {"input": "src/assets/themes/cranberry-pie/base-cranberry-pie.css", "bundleName": "cranberry-pie", "inject": false },
                            {"input": "src/assets/themes/cranberry-pie/dark-cranberry-pie.css", "bundleName": "dark-cranberry-pie", "inject": false },
                            {"input": "src/assets/themes/cranberry-pie/light-cranberry-pie.css", "bundleName": "light-cranberry-pie", "inject": false },
                            {"input": "src/assets/themes/gray-paper/base-gray-paper.css", "bundleName": "gray-paper", "inject": false },
                            {"input": "src/assets/themes/gray-paper/dark-gray-paper.css", "bundleName": "dark-gray-paper", "inject": false },
                            {"input": "src/assets/themes/gray-paper/light-gray-paper.css", "bundleName": "light-gray-paper", "inject": false },
                            {"input": "src/assets/themes/mellow-apricot/base-mellow-apricot.css", "bundleName": "mellow-apricot", "inject": false },
                            {"input": "src/assets/themes/mellow-apricot/dark-mellow-apricot.css", "bundleName": "dark-mellow-apricot", "inject": false },
                            {"input": "src/assets/themes/mellow-apricot/light-mellow-apricot.css", "bundleName": "light-mellow-apricot", "inject": false },
                            {"input": "src/assets/themes/pastel-red/base-pastel-red.css", "bundleName": "pastel-red", "inject": false },
                            {"input": "src/assets/themes/pastel-red/dark-pastel-red.css", "bundleName": "dark-pastel-red", "inject": false },
                            {"input": "src/assets/themes/pastel-red/light-pastel-red.css", "bundleName": "light-pastel-red", "inject": false },
                            {"input": "src/assets/themes/persian-green/base-persian-green.css", "bundleName": "persian-green", "inject": false },
                            {"input": "src/assets/themes/persian-green/dark-persian-green.css", "bundleName": "dark-persian-green", "inject": false },
                            {"input": "src/assets/themes/persian-green/light-persian-green.css", "bundleName": "light-persian-green", "inject": false },
                            {"input": "src/assets/themes/sea-blue/base-sea-blue.css", "bundleName": "sea-blue", "inject": false },
                            {"input": "src/assets/themes/sea-blue/dark-sea-blue.css", "bundleName": "dark-sea-blue", "inject": false },                            
                            {"input": "src/assets/themes/sea-blue/light-sea-blue.css", "bundleName": "light-sea-blue", "inject": false }
                        ],
                        "tsConfig": "tsconfig.app.json"
                    },
                    "configurations": {
                        "development": {
                            "optimization": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "extractLicenses": false,
                            "vendorChunk": true,
                            "buildOptimizer": false,
                            "budgets": []
                        },
                        "production-demo": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.production.demo.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "8mb",
                                    "maximumError": "10mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "production-live": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.production.live.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "8mb",
                                    "maximumError": "10mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "production-local": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.production.local.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "8mb",
                                    "maximumError": "10mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "FrontEnd:build"
                    },
                    "configurations": {
                        "development": {
                            "browserTarget": "FrontEnd:build:development"
                        },
                        "production-local": {
                            "browserTarget": "FrontEnd:build:production-local"
                        },
                        "production": {
                            "browserTarget": "FrontEnd:build:production"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "src/**/*.ts",
                            "src/**/*.html"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    }
}